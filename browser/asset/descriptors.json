{
	"animations": "animation/scene.json",
	"fonts": {
		"coffee": {
			"file": "font/coffee.json"
		}
	},
	"textures": {
		"spritesheet": {
			"file": "image/spritesheet.png"
		},
		"jonathan": {
			"file": "image/jonathan.jpg"
		}
	},
	"geometries": {
		"treeCurves": {
			"file": "mesh/treeCurves.obj"
		},
		"jonathan": {
			"file": "mesh/jonathan.obj"
		},
		"plantPoints": {
			"file": "mesh/plantPoints.ply"
		}
	},
	"shaderMaterials": {
		"filter": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/filter/filter.frag"
		},
		"gridScreen": {
			"vertexShader": "shader/project/gridScreen.vert",
			"fragmentShader": "shader/generic/color.frag"
		},
		"gridArrow": {
			"vertexShader": "shader/project/gridArrow.vert",
			"fragmentShader": "shader/generic/color.frag"
		},
		"opticalFlow": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/filter/opticalFlow.frag"
		},
		"opticalFrame": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/filter/opticalFrame.frag"
		},
		"feedback": {
			"vertexShader": "shader/generic/screen.vert",
			"fragmentShader": "shader/filter/feedback.frag"
		},
		"simple": {
			"vertexShader": "shader/generic/simple.vert",
			"fragmentShader": "shader/generic/simple.frag"
		},
		"ribbon": {
			"vertexShader": "shader/project/ribbon.vert",
			"fragmentShader": "shader/generic/color.frag",
			"side": 2
		},
		"snow": {
			"vertexShader": "shader/project/snow.vert",
			"fragmentShader": "shader/generic/circle.frag",
			"side": 2
		},
		"pointCloud": {
			"vertexShader": "shader/project/pointCloud.vert",
			"fragmentShader": "shader/generic/circle.frag",
			"side": 2
		},
		"curvedMesh": {
			"vertexShader": "shader/project/curvedMesh.vert",
			"fragmentShader": "shader/generic/texture.frag"
		},
		"lineMesh": {
			"vertexShader": "shader/project/lineMesh.vert",
			"fragmentShader": "shader/generic/color.frag",
			"side": 2
		},
		"spritesheet": {
			"vertexShader": "shader/project/sprite.vert",
			"fragmentShader": "shader/project/spritesheet.frag",
			"side": 2,
			"depthTest": false,
			"opacity": 0.5,
			"transparent": true
		}
	}
}
