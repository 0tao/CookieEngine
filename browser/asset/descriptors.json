{
	"animations": "animation/scene.json",
	"fonts": {
		"coffee": {
			"file": "font/coffee.json"
		}
	},
	"textures": {
		"spritesheet": {
			"file": "image/spritesheet.png"
		},
		"jonathan": {
			"file": "image/jonathan.jpg"
		}
	},
	"geometries": {
		"treeCurves": {
			"file": "mesh/treeCurves.obj"
		},
		"jonathan": {
			"file": "mesh/jonathan.obj"
		},
		"plantPoints": {
			"file": "mesh/plantPoints.ply"
		}
	},
	"shaderMaterials": {
		"filter": {
			"vertexShader": "shader/examples/filter.vert",
			"fragmentShader": "shader/examples/filter.frag"
		},
		"gridScreen": {
			"vertexShader": "shader/examples/gridScreen.vert",
			"fragmentShader": "shader/examples/color.frag"
		},
		"opticalFlow": {
			"vertexShader": "shader/examples/filter.vert",
			"fragmentShader": "shader/examples/opticalFlow.frag"
		},
		"opticalFrame": {
			"vertexShader": "shader/examples/filter.vert",
			"fragmentShader": "shader/examples/opticalFrame.frag"
		},
		"feedback": {
			"vertexShader": "shader/examples/filter.vert",
			"fragmentShader": "shader/examples/feedback.frag"
		},
		"simple": {
			"vertexShader": "shader/examples/simple.vert",
			"fragmentShader": "shader/examples/simple.frag"
		},
		"ribbon": {
			"vertexShader": "shader/examples/ribbon.vert",
			"fragmentShader": "shader/examples/color.frag",
			"side": 2
		},
		"snow": {
			"vertexShader": "shader/examples/snow.vert",
			"fragmentShader": "shader/examples/snow.frag",
			"side": 2
		},
		"pointCloud": {
			"vertexShader": "shader/examples/pointCloud.vert",
			"fragmentShader": "shader/examples/pointCloud.frag",
			"side": 2
		},
		"curvedMesh": {
			"vertexShader": "shader/examples/curvedMesh.vert",
			"fragmentShader": "shader/examples/curvedMesh.frag"
		},
		"lineMesh": {
			"vertexShader": "shader/examples/lineMesh.vert",
			"fragmentShader": "shader/examples/color.frag",
			"side": 2
		},
		"spritesheet": {
			"vertexShader": "shader/examples/sprite.vert",
			"fragmentShader": "shader/examples/spritesheet.frag",
			"side": 2,
			"depthTest": false,
			"opacity": 0.5,
			"transparent": true
		}
	}
}
